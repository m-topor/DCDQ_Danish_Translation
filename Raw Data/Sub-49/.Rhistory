)
cosData %>%
pivot_longer(cols = c("Post_QoL", "Base_QoL"), names_to = "name", values_to = "value") %>%
ggplot(aes(x = name, y = value, color = Surgery)) +
geom_point() +
geom_line(aes(group= ID)) +
facet_wrap(facets = vars(Reason))
